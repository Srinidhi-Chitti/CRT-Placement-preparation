# Leetcode 3 problems:
# 1) Reverse integer:
n = int(input())
rev = 0
while n! =0:
    digit = n%10
    rev = rev*10+digit
    n //=10

print(rev)
# 2) Factorial Trailing Zeroes
class Solution:
    def trailingZeroes(self, n: int) -> int:
        count = 0
        while n > 0:
            n //= 5
            count += n
        return count
